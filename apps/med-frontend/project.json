{
  "name": "med-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/med-frontend/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/med-frontend",
        "browser": "apps/med-frontend/src/main.ts",
        "tsConfig": "apps/med-frontend/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/med-frontend/public"
          }
        ],
        "styles": [
          "apps/med-frontend/src/styles.scss"
        ],
        "server": "apps/med-frontend/src/main.server.ts",
        "ssr": {
          "entry": "apps/med-frontend/src/server.ts"
        },
        "outputMode": "server"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "src/environments/environment.ts",
              "with": "src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "med-frontend:build:production"
        },
        "development": {
          "buildTarget": "med-frontend:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "med-frontend:build",
        "proxyConfig": "proxy.config.js"
      }
    },
    "lint": {
      "executor": "@angular-eslint/builder:lint",
      "options": {
        "lintFilePatterns": [
          "apps/med-frontend/**/*.ts",
          "apps/med-frontend/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/med-frontend/jest.config.cts",
        "tsConfig": "apps/med-frontend/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "med-frontend:build",
        "port": 4200,
        "staticFilePath": "dist/apps/med-frontend/browser",
        "spa": true
      }
    }
  }
}